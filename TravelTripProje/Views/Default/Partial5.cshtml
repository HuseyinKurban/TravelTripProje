@using TravelTripProje.Models.Siniflar
@model List<Blog>


<div class="box1-left">
    @foreach (var item in Model)
    {
        <div class="bot-img">
            <a href="/Blog/BlogDetay/@item.ID"> <img src="@item.BlogImage" alt=""  style="height:90px; width:180px;" /> </a>
        </div>
        <div class="bot-desc1">
            <p>@item.Aciklama.Substring(0, 100)...</p>
        </div>
        <div class="clear"></div>
    }
    </div>
