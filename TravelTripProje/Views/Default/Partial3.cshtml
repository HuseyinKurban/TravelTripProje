@using TravelTripProje.Models.Siniflar
@model List<Blog>

<div class="padding">
    <h4>En Popüler 10 Blog</h4>
    @{ int i = 0;}
    @foreach (var item in Model)
    {
        i++;
        <ul class="list list-pad">
            <li class="list-bg"><span>@i</span><a href="/Blog/BlogDetay/@item.ID">@item.Baslik</a></li>
        </ul>
    }

    <a href="#" class="link1">Tüm Otelleri Görün</a>
</div>
